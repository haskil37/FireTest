@model FireTest.Models.TeacherTest
@{
    ViewBag.Title = "Удалить тест";
}
<div class="row">
    <h3>Действительно хотите удалить тест под названием "@Model.NameTest"?</h3>
</div>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="row center">
        <input type="submit" value="Удалить" class="button" />
    </div>
    <div class="row center">
        @Html.ActionLink("На главную", "Index", null, new { @class = "button-Cancel" })
    </div>
}