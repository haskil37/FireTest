<div class="spoiler">
    <span class="headline">Выберите вид тестирования по дисциплинам</span>
</div>
<div class="-closed">
    @{ Html.RenderAction("Departments", "Home"); }
</div>

<div class="spoiler">
    <span class="headline">Ваша статистика</span>
</div>
<div class="-opened">
    <div class="row battle">
        <div class="col-2 col-offset-1">
        </div>
    </div>
</div>
<script>
    var images = [
       @Html.Raw(ViewBag.Images)
    ];
    var imagesLoad = [];
    for (var i = 0; i < images.length; i++) {
        imagesLoad[i] = new Image();
        imagesLoad[i].src = images[i];
    }
</script>