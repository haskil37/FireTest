@model List<FireTest.Models.Rating>
@{
    Layout = null;
}

<div class="row"><h3>Место в рейтинге группы: @ViewBag.RatingPosition</h3></div>
<div class="row">
    <ol start="@ViewBag.Start">
        @for (int i = 0; i < Model.Count(); i++)
        {
            if (ViewBag.You == i)
            {
                <li>
                    <img class="avatar-mini" src="@Model[i].Avatar" />
                    <span>@Model[i].Name @Model[i].Family</span>
                </li>
            }
            else
            {
                <li>
                    <img class="avatar-mini" src="@Model[i].Avatar" />
                    <span>@Model[i].Name @Model[i].Family</span>
                </li>
            }
        }
    </ol>
</div>